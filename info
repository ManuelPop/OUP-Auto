mvn clean -U dependency:unpack test -Dtest=TestSuiteOUP -Dwebdriver.driver=firefox -Dwebdriver.base.url=http://alfqa.oup.ixxus.co.uk/share -Dwebdriver.remote.url=http://localhost:5555/wd/hub -Dsurefire.rerunFailingTestsCount=1 -Dmax.retries=1 -DJUNIT_RETRY_TESTS=true -Dwebdriver.timeouts.implicitlywait=15000 -Dthucydides.timeout=30 -Dmaven.test.failure.ignore=true serenity:aggregate